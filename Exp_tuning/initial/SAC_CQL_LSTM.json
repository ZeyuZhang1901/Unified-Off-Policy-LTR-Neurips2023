{
    "actor_lr": 1e-4,
    "critic_lr": 1e-4,
    "alpha_lr": 1e-4,
    "cql_alpha_lr": 1e-4,
    "embed_lr": 1e-5,
    "lr_decay": "True",

    "batch_size": 256,
    "max_gradient_norm": 5.0,
    "target_update_steps": 50,
    "discount": 0.9,
    "l2_loss": 0.0,
    "metric_type": [
        "err",
        "ndcg"
    ],
    "metric_topn": [3,5,10],
    "objective_metric": "ndcg_10",

    "state_type": "avg_rew",
    "embed_type": "LSTM",
    "num_layer": 1,

    "auto_actor_alpha": "True",
    "initial_log_alpha": -6.0,

    "using_cql": "True",
    "with_lagrange": "True",
    "target_action_gap": 0.0,
    "initial_cql_alpha": 1.0

}